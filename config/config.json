        "auditor": {
          "enabled": true,
          "model": "anthropic/claude-opus-4-5",
          "temperature": 0.7,
          "maxTokens": 4096,
          "customPersona": ""
        },
        "researcher": {
          "enabled": true,
          "model": "google/gemini-3-flash-preview",
          "temperature": 0.7,
          "maxTokens": 4096,
          "customPersona": ""
        }
      },
      "qaGateEnabled": true,
      "auditGateEnabled": true,
      "auditThreshold": "sensitive",
      "deliberationTimeout": 120,
      "minOpinions": 3
    },
    "profiler": {
      "enabled": true,
      "interviewerModel": "anthropic/claude-opus-4-5",
      "autoInterview": true,
      "profileMaxAgeDays": 30,
      "quickAssessOnFailure": true,
      "storagePath": "~/.gigabot/profiles",
      "testTimeout": 30,
      "quickTestCategories": [
        "tool_calling",
        "instruction",
        "reasoning"
      ],
      "validateRoleAssignments": true,
      "applyGuardrails": true
    },
    "devWorkflow": {
      "enabled": false,
      "defaultPort": 3000,
      "devCommand": "npm run dev",
      "readyPattern": "ready|listening|started|compiled|Local:",
      "serverTimeout": 30,
      "visualValidation": {
        "enabled": false,
        "visionModel": "anthropic/claude-sonnet-4-5",
        "autoScreenshot": true,
        "maxFixIterations": 5,
        "screenshotDelayMs": 2000
      }
    },
    "memory": {
      "enabled": true,
      "vectorSearch": true,
      "contextMemories": 5,
      "autoExtractFacts": true,
      "saveCompactionSummaries": true,
      "storagePath": "~/.gigabot/memory",
      "vectorDimension": 384,
      "vectorWeight": 0.6,
      "keywordWeight": 0.3,
      "recencyWeight": 0.1,
      "recencyDays": 30
    },
    "selfHeal": {
      "enabled": true,
      "toolRetryEnabled": true,
      "maxToolRetries": 3,
      "retryBaseDelay": 1.0,
      "retryMaxDelay": 30.0,
      "retryExponentialBase": 2.0,
      "circuitBreakerEnabled": true,
      "circuitBreakerThreshold": 5,
      "circuitBreakerCooldown": 60,
      "swarmTaskRetry": true,
      "swarmMaxRetries": 2,
      "trackErrorPatterns": true,
      "errorPatternThreshold": 3
    },
    "toolReinforcement": {
      "enabled": true,
      "preValidation": true,
      "enforceSecurityPolicy": true,
      "adaptiveSelection": true,
      "trackModelToolPerformance": true,
      "feedbackToProfiler": true,
      "feedbackInterval": 100,
      "minCallsForConfidence": 5,
      "defaultConfidence": 0.7,
      "errorWarningThreshold": 3,
      "suggestAlternativeThreshold": 0.5,
      "advisorStoragePath": "~/.gigabot/tool_advisor.json"
    },
    "intentTracking": {
      "enabled": true,
      "analysisModel": "moonshot/kimi-k2.5",
      "trackPatterns": true,
      "patternAnalysisIntervalHours": 24,
      "maxHistoryDays": 90,
      "categories": [
        "work",
        "personal",
        "learning",
        "creative",
        "technical",
        "communication"
      ]
    },
    "memoryEvolution": {
      "enabled": true,
      "evolutionIntervalHours": 24,
      "autoPromote": true,
      "promotionAccessThreshold": 3,
      "promotionWindowDays": 7,
      "autoExpire": true,
      "decayInactiveDays": 30,
      "autoArchive": true,
      "archiveAfterDays": 90,
      "minImportanceThreshold": 0.1,
      "autoConsolidate": true,
      "consolidationThreshold": 0.85
