{
  "agents": {
    "defaults": {
      "workspace": "~/.nanobot/workspace",
      "model": "anthropic/claude-sonnet-4-5",
      "maxTokens": 8192,
      "temperature": 0.7,
      "maxToolIterations": 10
    },
    "tieredRouting": {
      "enabled": true,
      "tiers": {
        "dailyDriver": {
          "models": [
            "google/gemini-3-flash-preview",
            "anthropic/claude-sonnet-4-5"
          ],
          "triggers": [
            "chat",
            "simple",
            "quick"
          ]
        },
        "coder": {
          "models": [
            "anthropic/claude-sonnet-4-5",
            "openai/gpt-4o"
          ],
          "triggers": [
            "code",
            "debug",
            "implement",
            "fix"
          ]
        },
        "specialist": {
          "models": [
            "anthropic/claude-opus-4-5",
            "openai/gpt-4o"
          ],
          "triggers": [
            "analyze",
            "research",
            "complex",
            "architect"
          ]
        }
      },
      "classifierModel": "",
      "fallbackTier": "daily_driver"
    },
    "swarm": {
      "enabled": true,
      "maxWorkers": 5,
      "workerModel": "anthropic/claude-sonnet-4-5",
      "orchestratorModel": "anthropic/claude-sonnet-4-5",
      "autoTrigger": true,
      "complexityThreshold": 3,
      "patterns": {
        "research": [
          "search",
          "summarize",
          "report"
        ],
        "code": [
          "implement",
          "test",
          "refactor"
        ],
        "review": [
          "analyze",
          "critique",
          "suggest"
        ],
        "brainstorm": [
          "generate",
          "evaluate",
          "develop"
        ]
      }
    },
    "team": {
      "enabled": true,
      "roles": {
        "architect": {
          "enabled": true,
          "model": "anthropic/claude-opus-4-5",
          "temperature": 0.7,
          "maxTokens": 4096,
          "customPersona": ""
        },
        "leadDev": {
          "enabled": true,
          "model": "anthropic/claude-sonnet-4-5",
          "temperature": 0.7,
          "maxTokens": 4096,
          "customPersona": ""
        },
        "seniorDev": {
          "enabled": true,
          "model": "moonshot/kimi-k2.5",
          "temperature": 0.7,
          "maxTokens": 4096,
          "customPersona": ""
        },
        "juniorDev": {
          "enabled": true,
          "model": "google/gemini-3-flash-preview",
          "temperature": 0.7,
          "maxTokens": 4096,
          "customPersona": ""
        },
        "qaEngineer": {
          "enabled": true,
          "model": "anthropic/claude-sonnet-4-5",
          "temperature": 0.7,
          "maxTokens": 4096,
          "customPersona": ""
