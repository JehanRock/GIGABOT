    "costOptimization": {
      "enabled": true,
      "responseCaching": true,
      "cacheMaxSize": 1000,
      "cacheTtlSeconds": 3600,
      "cacheSimilarityThreshold": 0.95,
      "dailyBudgetUsd": 0.0,
      "weeklyBudgetUsd": 0.0,
      "alertThreshold": 0.8,
      "autoDowngradeOnBudget": false,
      "contextPruning": true,
      "suggestModelDowngrades": true,
      "cacheStoragePath": "~/.gigabot/cache.json"
    },
    "proactive": {
      "enabled": true,
      "maxDailyActions": 10,
      "requireConfirmation": true,
      "enableReminders": true,
      "enableSuggestions": true,
      "enableAutomation": false,
      "enableInsights": true,
      "enableAnticipation": true,
      "automationAllowlist": [],
